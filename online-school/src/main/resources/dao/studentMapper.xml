<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xxp.mapper.StudentMapper">

	
	
	<select id="getStudentByName" parameterType="String" resultType="xxp.entity.Student">
		select *
		from student
		where studentName = #{studentName}
	</select>
	
	

	
	
</mapper>